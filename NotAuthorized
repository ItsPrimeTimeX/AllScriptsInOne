-- Scripts:

local function QJPNSP_fake_script() -- TextLabel_4.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_4)

	local textLabel = script.Parent -- Replace "script.Parent" with the reference to your TextLabel
	
	local countdownDuration = 10 -- Duration of the countdown in seconds
	
	-- Function to update the TextLabel text with the countdown
	local function updateCountdown()
		for countdown = countdownDuration, 0, -1 do
			textLabel.Text = "Closing & Alerting Owner in: " .. tostring(countdown) -- Update the TextLabel text with the countdown value
	
			-- Wait for 1 second
			wait(1)
		end
	
		textLabel.Text = "Alerted!" -- Update the TextLabel text after the countdown finishes
		script.Parent.Parent.Parent:Destroy() -- Make script.Parent.Parent invisible
	end
	
	-- Start the countdown
	updateCountdown()
end
coroutine.wrap(QJPNSP_fake_script)()
